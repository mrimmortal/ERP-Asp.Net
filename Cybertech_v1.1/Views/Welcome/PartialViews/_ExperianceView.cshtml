@model Cybertech_v1._1.Models.ExperianceModel

<div id="close"></div>
<div class="row">
    <div class="col-6">
        <div class="form-group row">
            <div class="col">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">Employer</div>
                    </div>
                    <input id="Employer" name="Employer" type="text" class="form-control">
                </div>
            </div>
        </div>

        <div class="form-group row">
            <div class="col">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">Position</div>
                    </div>
                    <input id="Position" name="Position" type="text" maxlength="20" class="form-control">
                </div>
            </div>
        </div>

        <div class="form-group row">
            <div class="col-6">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">From</div>
                    </div>
                    <input id="From" name="From" type="month" class="form-control">
                </div>
            </div>

            <div class="col-6">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">To</div>
                    </div>
                    <input id="To" name="To" type="month" class="form-control">
                </div>
            </div>
        </div>
    </div>
    <div class="col-6">
        <div class="form-group row">
            <div class="col">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">Principle Activites</div>
                    </div>
                    <textarea id="PrincipleActivity" name="PrincipleActivity" type="text" class="form-control"> </textarea>
                </div>
            </div>
        </div>

        <div class="form-group row">
            <div class="col">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">Key Achievement</div>
                    </div>
                    <textarea id="KeyAchievement" name="KeyAchievement" class="form-control"></textarea>
                </div>
            </div>
        </div>
        @*<div class="row pr-5">
                ID : @Html.TextBoxFor(x => x.RegisterId, new { type="date", @class = "form-control form-control-sm" })
          </div>*@
    </div>
</div>
<hr />
