    <script>
        //Clone Experiance Div And Add Remove Button
        var uniqueId = 1;
        $(document).ready(function () {
            $('#add').click(function () {
                var copy = $("#experianceDiv").clone(true);
                var formId = 'Div' + uniqueId;
                copy.attr('id', formId);

                var removeButton = $('<input>', {
                    type: 'button',
                    val: 'x',
                    class: 'btn btn-danger pull-right',
                    id: formId
                });

                copy.prepend(removeButton);
                $('#cloneDiv').append(copy);

                uniqueId++;
            });

            $('#cloneDiv').on('click', '.btn-danger', function () {
                //$(this).parentNode.remove();
                $(this).parent('div').remove();
            });
        });
    </script>


    <script>
        $(document).ready(function () {

            var inputs = $("#NewRegistration input[required='required']");
            var selects = $("#NewRegistration select[required='required']")
            $('#submitform').click(function () {

                selects.each(function () {

                    if ($(this).val() == "") {

                        var current = $(this).closest(".collapse");

                        if (!current.hasClass("in")) {
                            current.collapse("show");
                        }
                        return false;
                    }
                });

                inputs.each(function () {

                    if ($(this).val() == "") {

                        var current = $(this).closest(".collapse");

                        if (!current.hasClass("in")) {
                            current.collapse("show");
                        }

                        return false;
                    }
                });
            });
        });
    </script>

